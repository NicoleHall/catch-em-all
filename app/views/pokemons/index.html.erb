<div class="row">
  <% @pokemons.each do |pokemon| %>
    <div class="col-md-3 text-center pokemon-container">
      <%= image_tag(pokemon.image_url, class: "pokemon-image") %>
      <h3><%= pokemon.name %></h3>
      <%= button_to "Capture", backpack_pokemons_path(pokemon_id: pokemon.id) %>
    </div>
  <% end %>
</div>


<!-- button_to expects to POST to somewhere
but you don't want to post to the create,
you are not creating anything
all you want to do is update backpack pokemons session -->
